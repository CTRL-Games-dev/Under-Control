            "Values": [
                {"Lang": "en", "Value": "Hello, Allseeing."},
                {"Lang": "pl", "Value": "Witaj, Wszechwidzący."}
            ]
        },
        {
            "Key": "dialogue_wszechwiedzacy_monolog_1_key",
            "Values": [
            {"Lang": "en", "Value": "I am the Allseeing. I see everything."},
            {"Lang": "pl", "Value": "Jestem Wszechwidzącym. Widzę wszystko."}
            ]
        },
        {
            "Key": "dialogue_wszechwiedzacy_monolog_2_key",
            "Values": [
            {"Lang": "en", "Value": "Knowledge is power, and I hold it all %PlayerName%."},
            {"Lang": "pl", "Value": "Wiedza to potęga, a ja posiadam ją całą."}
            ]
        },
        {
