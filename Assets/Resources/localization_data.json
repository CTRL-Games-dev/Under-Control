            "Values": [
            {"Lang": "en", "Value": "The truth is hidden in plain sight."},
            {"Lang": "pl", "Value": "Prawda jest ukryta na widoku."}
            ]
        },
        {
            "Key": "dialogue_wszechwiedzacy_monolog_4_key",
            "Values": [
            {"Lang": "en", "Value": "Do you seek answers, traveler?"},
            {"Lang": "pl", "Value": "Czy szukasz odpowiedzi, podróżniku?"}
            ]
        },
        {
            "Key": "dialogue_wszechwiedzacy_monolog_5_key",
            "Values": [
            {"Lang": "en", "Value": "Beware, for knowledge comes at a price."},
            {"Lang": "pl", "Value": "Strzeż się, bo wiedza ma swoją cenę."}
            ]
